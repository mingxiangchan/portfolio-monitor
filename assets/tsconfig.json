{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "target": "ES5",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "ES2015",
    "moduleResolution": "node",
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules"
  ]
}
